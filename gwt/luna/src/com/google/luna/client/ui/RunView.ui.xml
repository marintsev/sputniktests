<ui:UiBinder 
    xmlns:ui='urn:ui:com.google.gwt.uibinder'
    xmlns:g='urn:import:com.google.gwt.user.client.ui'
    xmlns:l='urn:import:com.google.luna.client.ui'>

  <ui:style src="RunView.css" />

  <g:FlowPanel styleName='{style.outer}'>
    <l:TestControlPanel ui:field='controls' styleName='{style.controls}'/>
    <l:ResultList ui:field='results' addStyleNames='{style.results}'/>
    <g:FlowPanel ui:field='workspace' styleName='{style.workspace}'/>
  </g:FlowPanel>

</ui:UiBinder>

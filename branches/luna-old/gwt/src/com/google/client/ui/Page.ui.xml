<ui:UiBinder 
    xmlns:ui='urn:ui:com.google.gwt.uibinder'
    xmlns:g='urn:import:com.google.gwt.user.client.ui'
    xmlns:s='urn:import:com.google.client.ui'>
  <ui:style src="Page.css" />

  <g:HTMLPanel styleName='{style.broadCenter}'>
	<g:VerticalPanel>
	  <g:Label ui:field="title" styleName='{style.title}'/>
	  <g:Label ui:field="subtitle" styleName='{style.subtitle}'/>
	  <g:HorizontalPanel>
	  	<s:TabButton ui:field='aboutButton' styleName='{style.tabButton}'/>
	  	<s:TabButton ui:field='runButton' styleName='{style.tabButton}'/>
	  	<s:TabButton ui:field='compareButton' styleName='{style.tabButton}'/>
	  </g:HorizontalPanel>
	  <s:FramePanel styleName='{style.contents}'>
        <s:PageContents ui:field="contents" />
      </s:FramePanel>
	</g:VerticalPanel>
  </g:HTMLPanel>

</ui:UiBinder>

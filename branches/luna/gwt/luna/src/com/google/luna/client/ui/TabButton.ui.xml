<ui:UiBinder 
    xmlns:ui='urn:ui:com.google.gwt.uibinder'
    xmlns:g='urn:import:com.google.gwt.user.client.ui'
    xmlns:l='urn:import:com.google.luna.client.ui'>
  
  <ui:style src='TabButton.css'/>

  <g:HTMLPanel>
    <l:FramePanel styleName='{style.frame}' emphasisOnHover="true">
      <g:FlowPanel styleName='{style.middle}'>
        <g:Label ui:field='label' styleName='{style.text}'/>
      </g:FlowPanel>
    </l:FramePanel>
  </g:HTMLPanel>

</ui:UiBinder>

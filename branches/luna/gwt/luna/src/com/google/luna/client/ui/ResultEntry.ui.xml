<ui:UiBinder 
    xmlns:ui='urn:ui:com.google.gwt.uibinder'
    xmlns:g='urn:import:com.google.gwt.user.client.ui'
    xmlns:l='urn:import:com.google.luna.client.ui'>

  <ui:with field='resources' type='com.google.luna.client.ui.ResultEntry.Resources'/>

  <g:HTMLPanel styleName='{resources.css.outer}'>
    <l:PromiseLabel ui:field='name' styleName='{resources.css.name}'/> - <l:PromiseLabel ui:field='description' styleName='{resources.css.description}'/>
  </g:HTMLPanel>

</ui:UiBinder>

<ui:UiBinder 
    xmlns:ui='urn:ui:com.google.gwt.uibinder'
    xmlns:g='urn:import:com.google.gwt.user.client.ui'
    xmlns:l='urn:import:com.google.luna.client.widget'>

  <ui:with field='resources' type='com.google.luna.client.widget.RunView.Resources'/>

  <g:FlowPanel styleName='{resources.css.outer}'>
    <l:TestControlPanel ui:field='controls' addStyleNames='{resources.css.controls}'/>
    <l:ResultList ui:field='results' addStyleNames='{resources.css.results}'/>
    <g:FlowPanel ui:field='workspace' styleName='{resources.css.workspace}'/>
  </g:FlowPanel>

</ui:UiBinder>
